        #include<iostream>
        #include<math.h>
        using namespace std;
             
        int main(){
        	
        	int cases, base;
            	cin>>cases;
            	
            	while(cases--){
            		
            		cin>>base;
            		base=(base/2)-1;
            		base=(base*(base+1))/2;
            		cout<<base<<endl;
            	}
            	return 0;
        }    