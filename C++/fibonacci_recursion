//find th nth term of fibonacci using recursion it is quite slow and consumes memory.
#nclude<iostream>
using namespace std;
int fib(int n){
	if (n==0){
		return 0;
	}
	else if(n==1){
		return 1;
	}
	
	return fib(n-1) + fib(n-2);
	
}

int main(){
	int b;
	
		
		cout<<"Enter the number= ";
		cin>>b;
		
		cout<<fib(b)<<endl;
	
return 0;
}
