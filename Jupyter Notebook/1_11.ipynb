      "between : 1\n",
      "choosing : 1\n",
      "or : 2\n",
      "to : 1\n"
     ]
    }
   ],
   "source": [
    "a = input()\n",
    "a = a.split()\n",
    "a.sort()\n",
    "n = len(a)\n",
    "count = 1\n",
    "for i in range (n-1):\n",
    "    if a[i] == a[i+1]:\n",
    "        count += 1\n",
    "    else:\n",
    "        print(str(a[i]) + \" : \" + str(count))\n",
    "        count = 1\n",
    "if a[n-1] != a[n-2]:\n",
    "    print(str(a[n-1]) + \" : 1\")"
   ]
  }
