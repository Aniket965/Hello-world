%TITLE 'helloworld'
MODULE helloworld (IDENT='V1.0', MAIN=helloworld,
        ADDRESSING_MODE (EXTERNAL=GENERAL)) =
BEGIN

    LIBRARY 'SYS$LIBRARY:STARLET';

    EXTERNAL ROUTINE
       LIB$PUT_OUTPUT;

GLOBAL ROUTINE helloworld =
BEGIN
    LIB$PUT_OUTPUT(%ASCID %STRING('Hello world!'))
END;

END
ELUDOM
