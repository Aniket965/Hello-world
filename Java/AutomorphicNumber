public class Automorph
{
public void Automorph(int n)
{
int c=0,p,k=0;c=0;
double r;
int m=n;
p=m*m;
    
while(n!=0)
{
k=n/10;
c=c+1;
n=k;
}
//return(c);
r=p%(Math.pow(10,c));
if(m==r)
System.out.println(m+ " is an automorphic number ");
else
System.out.println(m+ " is not an automorphic number ");
}
}
